{
  "name": "ro-b-ot",
  "description": "Ro嗶ot：一隻設計來把嗶諾搞瘋的 Discord 機器人",
  "version": "1.0.3",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "check": "biome check --fix",
    "dev": "node --experimental-strip-types --env-file=.env --watch src/index.ts",
    "test": "node --experimental-strip-types --env-file=.env --test tests/*.ts",
    "start": "node --experimental-strip-types src/index.ts"
  },
  "dependencies": {
    "discord.js": "14.25.1",
    "emoji-toolkit": "9.0.1",
    "lowdb": "7.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/node": "24.10.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome"
    ]
  }
}
